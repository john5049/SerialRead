<Window x:Class="SerialRead.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SerialRead"
        mc:Ignorable="d" Name="mainWindow"
        Title="MainWindow" Height="600" Width="1200" Loaded="Window_Loaded">
    <Window.Resources>
        <local:viewModel x:Key="viewModel"></local:viewModel>
    </Window.Resources>
    <Grid Background="#FF262626">
        <local:InstrumentCluster></local:InstrumentCluster>

    </Grid>
</Window>
